{
	"name": "commonplace-kerrbear",
	"version": "0.3.7",
	"license": "MIT",
	"description": "A zettelkasten-style digital garden full of half-baked poetry and other musings.",
	"repository": {
		"git": "git@github.com:krry/commonplace.git"
	},
	"scripts": {
		"build": "next build",
		"deploy:prod": "netlify deploy --prod",
		"deploy": "netlify deploy",
		"dev:ssl": "node server.js",
		"dev:watch": "next-remote-watch ./_notes",
		"dev": "NODE_OPTIONS='--inspect' next dev",
		"lint": "eslint ./ --fix --ext js",
		"maintain": "yarn lint && yarn search && yarn rss",
		"prepare": "husky install",
		"prod": "yarn release && netlify deploy --prod",
		"rehearse": "yarn build && yarn maintain",
		"rss": "node ./lib/rss.js",
		"search": "node lib/cache.js",
		"stage": "yarn rehearse && netlify deploy",
		"start": "next start"
	},
	"dependencies": {
		"@emotion/react": "^11.1.5",
		"dotenv": "^9.0.2",
		"fs-readdir-recursive": "^1.1.0",
		"fuse.js": "^6.4.6",
		"fuzzy-search": "^3.2.1",
		"gray-matter": "4.0.2",
		"husky": ">=6",
		"next": "^10.0.9",
		"next-auth": "^3.13.3",
		"next-mdx-remote": "^2.1.3",
		"next-pwa": "^5.2.18",
		"next-themes": "^0.0.14",
		"next-tinacms-github": "^0.41.0",
		"postcss-color-mod-function": "^3.0.3",
		"prism-react-renderer": "^1.2.0",
		"react": "17.0.1",
		"react-dom": "17.0.1",
		"react-hotkeys-hook": "^3.3.0",
		"react-icons": "^4.2.0",
		"react-spinners": "^0.10.6",
		"react-tinacms-editor": "^0.41.0",
		"react-tinacms-github": "^0.41.0",
		"react-tinacms-inline": "^0.41.0",
		"rss": "^1.2.2",
		"styled-components": "^5.3.0",
		"stylelint": "^13.12.0",
		"stylelint-config-prettier": "^8.0.2",
		"superjson": "^1.7.3",
		"tina-graphql-gateway": "^0.2.10",
		"tinacms": "^0.41.0"
	},
	"devDependencies": {
		"@types/node": "^15.3.0",
		"@types/react": "^17.0.5",
		"babel-plugin-superjson-next": "^0.2.3",
		"eslint": "^7.24.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-mdx": "^1.12.0",
		"eslint-plugin-prettier": "^3.4.0",
		"eslint-plugin-react": "^7.23.2",
		"lint-staged": ">=10",
		"next-remote-watch": "^1.0.0",
		"postcss-advanced-variables": "^3.0.1",
		"postcss-preset-env": "^6.7.0",
		"prepend-file": "^2.0.0",
		"prettier": "^2.2.1",
		"remark-external-links": "^8.0.0",
		"remark-images": "^2.0.0",
		"remark-inline-links": "5.0.0",
		"remark-numbered-footnote-labels": "^1.1.0",
		"remark-preset-prettier": "^0.4.1",
		"remark-unwrap-images": "^2.0.0",
		"remark-wiki-link": "1.0.2",
		"stylelint-config-standard": "^21.0.0",
		"tina-graphql-gateway-cli": "^0.2.41",
		"typescript": "^4.2.4"
	},
	"lint-staged": {
		"*.js": "eslint --cache --fix"
	}
}
