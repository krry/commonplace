{
	"name": "commonplace",
	"version": "0.2.7",
	"license": "CC-BY-NC-SA-4.0",
	"repository": {
		"git": "git@github.com:krry/commonplace.git"
	},
	"scripts": {
		"dev": "NODE_OPTIONS='--inspect' next dev",
		"dev:ssl": "node server.js",
		"dev:watch": "next-remote-watch ./_notes",
		"build": "next build",
		"lint": "eslint ./ --fix --ext js",
		"cache": "node lib/cache.js",
		"start": "next start"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn cache && git add _cache/data.js && yarn lint",
			"pre-push": "yarn build"
		}
	},
	"dependencies": {
		"@fontsource/inconsolata": "^4.2.2",
		"@fontsource/museomoderno": "^4.2.2",
		"@fontsource/quicksand": "^4.2.2",
		"fs-readdir-recursive": "^1.1.0",
		"fuse.js": "^6.4.6",
		"fuzzy-search": "^3.2.1",
		"gray-matter": "4.0.2",
		"husky": "^6.0.0",
		"next": "^10.0.9",
		"next-auth": "^3.13.3",
		"next-mdx-remote": "^2.1.3",
		"next-themes": "^0.0.14",
		"prism-react-renderer": "^1.2.0",
		"react": "17.0.1",
		"react-dom": "17.0.1",
		"react-hotkeys-hook": "^3.3.0",
		"react-icons": "^4.2.0",
		"stylelint": "^13.12.0",
		"superjson": "^1.7.3"
	},
	"devDependencies": {
		"babel-plugin-superjson-next": "^0.2.3",
		"eslint": "^7.24.0",
		"eslint-plugin-react": "^7.23.2",
		"next-remote-watch": "^1.0.0",
		"postcss-advanced-variables": "^3.0.1",
		"postcss-preset-env": "^6.7.0",
		"prepend-file": "^2.0.0",
		"remark-external-links": "^8.0.0",
		"remark-images": "^2.0.0",
		"remark-inline-links": "5.0.0",
		"remark-numbered-footnote-labels": "^1.1.0",
		"remark-unwrap-images": "^2.0.0",
		"remark-wiki-link": "1.0.2",
		"stylelint-config-standard": "^21.0.0"
	}
}
